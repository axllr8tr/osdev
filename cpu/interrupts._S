/* to be used later */
.extern interrupt_handler_generic
.global interrupt_handler_generic_entry
interrupt_handler_generic_entry:
  pushal
  push %ds
  push %es
  push %fs
  push %gs 
  movl $0x10, %eax
  movl %eax, %ds 
  movl %eax, %es 
  movl %eax, %fs
  movl %eax, %gs
  movl %esp, %edi 
  call interrupt_handler_generic 
  popl %eax 
  pop %gs
  pop %fs
  pop %es
  pop %ds
  popal
  iret

.extern interrupt_handler_err
.global interrupt_handler_err_entry
interrupt_handler_err_entry:
  pushal
  push %ds 
  push %es
  push %fs
  push %gs
  movl $0x10, %eax
  movl %eax, %ds 
  movl %eax, %es 
  movl %eax, %fs
  movl %eax, %gs
  pushl %esp
  call interrupt_handler_err
  popl %eax 
  pop %gs
  pop %fs
  pop %es
  pop %ds
  popal
  iret
